		<value>Patna</value>
	</util:set>
	<util:map map-class="java.util.HashMap" id="course">
		<entry key="Java" value="1 Year" />
		<entry key="dot net" value="2 Year" />
		<entry key="ROR" value="3 Year" />
	</util:map>
	<util:properties id="importantkeys">
		<prop key="userName">vvarunbit@gmail.com</prop>
		<prop key="password">admin@123</prop>
	</util:properties>

